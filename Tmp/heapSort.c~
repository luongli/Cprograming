#include<stdio.h>
#include<time.h>
#include"/home/li/Cprograming/Library/luong.h"

//prototype
void maxHeapify(int *A, int i, int n);



int main()
{

  char name[] = "data10.txt";
  int len = 10;
  int A[10];

  srand(time(NULL));

  generateData(A, name, 0, 100, 10);

  

}

void maxHeapify(int *A, int i, int n)
{

  int l = 2*i;
  int r = 2*i+1;
  int max;

  if((l <= n) && (A[l] > A[i])){
    max = l;
  }else max = i;

  if((r <= n) && (A[r]) > A[max]){
    max = r;
  }

  if(max != i){
    swap(&A[i], &A[max]);
    maxHeapify(A, max, n);
  }

}//end maxHeapify
